for i in range(5):
    for j in range(5):
        print("* ",end='')
    print()
    
    
    
for i in range(1,5):
    for j in range(1,6):
        if i==1 or i==4 or j==1 or j==5:
            print("* ",end='')
        else:
            print("  ",end='')
    print()
    
    
    
#打印一个菱形



for i in range(1,6):
    for j in range(1,7-i):
        print(" ",end='')
    for k in range(6-i,6):
        print("* ",end='')
    print()
    
    
    
for i in range(1,5):
    for j in range(1,7-i):
        print(" ",end='')
    for k in range(6-i,6):
        print("* ",end='')
    print()
for i in range(1,6):
    for j in range(i):
        print(" ",end='')
    for k in range(6-i):
        print("* ",end='')
    print()
    
    
    
for i in range(1,5):
    for j in range(1,7-i):
        print(" ",end='')
    for k in range(6-i,6):
        if i==1 or j==6-i or j==5:
            print("* ",end='')
        else:
            print('',end='')
    print()
for i in range(1,6):
    for j in range(i):
        print(" ",end='')
    for k in range(6-i):
        if i==5 or j==6 or 6-i-1:
            print("* ",end='')
        else:
            print("  ",end='')
    print()
    
    
    
for i in range(1,5):
    for k in range(1,7-i):
        print(end=" ")
    for j in range(6-i,6):
        if i==1 or j==6-i or j==5:
            print("* ",end="")
        else:
            print(' ',end=" ")
    print()
for i in range(1,6):
    for k in range(i):
        print(end=" ")
    for j in range(6-i):
        if i==5 or j==0 or j==6-i-1:
            print("* ",end="")
        else:
            print(" ",end=" ")
    print()
    
    
    
