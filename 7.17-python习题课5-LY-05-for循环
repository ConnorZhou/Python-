            if k==0 or k==4:
                print("* ",end='')
            if k==1 or k==2 or k==3:


for i in range(0,4):
    if i==0 or i==3:
        print("* "*5)
    if i==1 or i==2:
        for k in range(0,5):
            if k==0 or k==4:
                print("* ",end='')
            else:
                print("  ",end='')
            print()
    
for i in range(0,4):
    if i==0 or i==3:
        print("* "*5)
    if i==1 or i==2:
        for k in range(0,5):
            if k==0 or k==4:
                print("* ",end='')
            else:
                print("  ",end='')
        print()


for i in range(5):
    print("* "*(i+1))


for i in range(5):
    for j in range(i+1):
        print("* ",end='')
    print()
    
for i in range(5):
    if i==0 or i==1 or i==4:
        print("* "*(i+1))
    if i==2:
        for j in range(2):
            if j==0 or j==2:
                print("* ",end='')
            else:
                print("  ",end='')
        print()
    if i==3:
        for j in range(3):
            if j==0 or j==3:
                print("* ",end='')
            else:
                print("  ",end='')
                
                
for i in range(5):
    if i==0 or i==1 or i==4:
        print("* "*(i+1))
    else:
        for j in range(i+1):
            if j==0 or j==i:
                print("* ",end='')
            else:
                print("  ",end='')
        print()
        
        
for i in range(5):
