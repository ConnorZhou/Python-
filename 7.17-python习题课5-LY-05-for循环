        print("* "*(i+1))
    else:
        for j in range(i+1):
            if j==0 or j==i:
                print("* ",end='')
            else:
                print("  ",end='')
        print()
        
        
for i in range(5):
    if i==0 or i==4:
        print("* "*(i+1))
    else:
        for j in range(i+1):
            if j==0 or j==i:
                print("* ",end='')
            else:
                print("  ",end='')
        print()
        
        
for i in range(5):
    for j in range(i+1):
        if i==4:
            print("* ",end='')
            continue
        if j==0 or j==i:
            print("* ",end='')
        else:
            print("  ",end='')
    print()
    
    
for i in range(5):
    print("* "*(5-i))
    
    
for i in range(5):
    for j in range(5-i):
        print("* ",end='')
    print()
    
    
for i in range(5):
    if i==0 or i==4:
        print("* "*(5-i))
    else:
        print("* ","  "*(5-2-i),"* ")
        
        
for i in range(5):
    if i==0 or i==4:
        print("* "*(5-i))
    else:
        print("* "," "*(5-2-i),"* ")
        
        
for i in range(5):
    if i==0 or i==4:
        print("* "*(5-i))
    else:
        for j in range(5-i):
            if j==0 or j==(5-1-i):
                print("* ",end='')
            else:
                print("  ",end='')
        print()
        
        
for i in range(5):
    for j in range(5-i):
        if i==0:
            print("* ",end='')
            continue
        else:
            if j==0 or j==(5-i-1):
                print("* ",end='')
            else:
                print("  ",end='')
    print()
    
    
for i in range(5):
    for j in range(5-i-1):
        print(" ",end='')
    for m in range(i+1):
        print("* ",end='')
    print()
    
    
for i in range(5):
    for j in range(5-i-1):
        print(" ",end='')
    for m in range(i+1):
        if m==0 or m==i:
            print("* ",end='')
        else:
            print("  ",end='')
    print()
    
    
for i in range(1,6):
    for k in range(1,6-i):
        print(end=' ')
    for j in range(6-i,6):
        if i==5 or j==6-i or j==5:
            print("*",end=' ')
        else:
            print("  ",end='')
    print()
    
    
