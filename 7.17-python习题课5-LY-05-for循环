for i in range(4):
    print("* "*5)


for i in range(0,4):
    for k in range(0,5):
        print("* ",end='')
        print(end='')
    print()


for i in range(0,4):
    if i==0 or i==3:
        print("* "*5)
    if i==1 or i==2:
        for k in range(0,5):
            if k==0:
                print("* ",end='')
            if k==1:
                print("  ",end='')
            if k==2:
                print("  ",end='')
            if k==3:
                print("  ",end='')
            if k==4:
                print("* ")
 
