class Animal():
    def __init__(self):
        print("Animal")
class PaxingAni(Animal):
    def __init__(self,name):
        print("Paxing Dongwu {0}".format(name))
class Dog(PaxingAni):
    def __init__(self):
        print("I am init in dog")
d=Dog()
class Cat(PaxingAni):
    pass
c=Cat("JSFHJK")



